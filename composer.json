{
  "name": "cyrosy/php-end-of-life",
  "description": "Library exposing EOL dates of PHP versions",
  "license": "EUPL-1.2",
  "type": "library",
  "version": "1.2.0",
  "authors": [
    {
      "name": "Cyril SAINT-YVES",
      "email": "github@cyrosy.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cyrosy\\PhpEndOfLife\\": "src"
    }
  },
  "autoload-dev": {
    "Cyrosy\\PhpEndOfLife\\Test": "tests",
    "Cyrosy\\PhpEndOfLife\\TestCompat": "tests-compat"
  },
  "require": {
    "php": "^5.3.3|^7.0|^8.0",
    "webmozart/assert": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "bin": [
    "bin/end-of-life"
  ],
  "config": {
    "sort-packages": true
  }
}
